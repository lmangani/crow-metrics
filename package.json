{
  "name": "crow-metrics",
  "version": "3.0.0",
  "description": "small metrics collector for node.js servers",
  "keywords": [
    "metrics",
    "prometheus",
    "servo"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:robey/crow-metrics.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",

{
  "name": "crow-metrics",
  "version": "1.0.7",
  "description": "small metrics collector for node",
  "keywords": [
    "metrics",
    "prometheus",
    "servo",
    "graphs"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:robey/node-crow.git"
  },
  "licenses": [
    {
      "type": "Apache License 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.txt"
    }
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "prepublish": "babel --source-maps --out-dir lib src && babel --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib --timeout 30000"

{
  "name": "crow-metrics",
  "version": "2.0.0",
  "description": "small metrics collector for node",
  "keywords": [
    "metrics",
    "prometheus",
    "servo"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:robey/node-crow.git"
  },
  "licenses": [
    {
      "type": "Apache License 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.txt"
    }
  ],
  "scripts": {
    "build": "eslint src test/src && npm run build-source && npm run build-test",
    "build-source": "babel --source-maps --out-dir lib src",
    "build-test": "babel --source-maps --out-dir test/lib test/src",
    "clean": "rm -rf lib",
    "prepublish": "npm run build",
    "test": "mocha -R spec --colors test/lib --timeout 30000"
  },
  "dependencies": {
    "bluebird": "latest"
  },
  "devDependencies": {
    "babel": "^6.0.0",
    "babel-cli": "^6.0.0",
    "babel-preset-es2015-node4": "^1.0.0",
    "eslint": "^1.4.1",
    "mocha": "*",
    "should": "*",
    "source-map-support": "*"
  },
  "main": "./lib/crow",
  "files": [
    "doc",
    "lib",
    "static"
  ]
}
